<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:gravity="center_horizontal"
    android:orientation="vertical" >
<LinearLayout
    android:id="@+id/linearlayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    >
    <include
        android:id="@+id/layout_bar"
        layout="@layout/layout_title" />

    <include
        android:id="@+id/layout_header"
        layout="@layout/layout_header" />

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >
            <TextView
                android:id="@+id/txt_zcgl"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/setting_item_selector"
                android:drawableLeft="@drawable/img_zcgl"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/right"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:text="@string/zcgl"
                android:textSize="16dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:background="@color/black2" />

            <TextView
                android:id="@+id/txt_tzgl"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/setting_item_selector"
                android:drawableLeft="@drawable/img_tzgl"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/right"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:text="@string/tzgl"
                android:textSize="16dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:background="@color/black2" />

            <TextView
                android:id="@+id/txt_jlcx"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/setting_item_selector"
                android:drawableLeft="@drawable/img_jlcx"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/right"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:text="@string/jlcx"
                android:textSize="16dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:background="@color/black2" />

            <TextView
                android:id="@+id/txt_wdyhk"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/setting_item_selector"
                android:drawableLeft="@drawable/img_wdyhk"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/right"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:text="@string/wdyhk"
                android:textSize="16dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:background="@color/black2" />

            <TextView
                android:id="@+id/txt_wdxx"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/setting_item_selector"
                android:drawableLeft="@drawable/img_wdxx"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/right"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:text="@string/wdxx"
                android:textSize="16dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:background="@color/black2" />

            <TextView
                android:id="@+id/txt_myredpager"
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:background="@drawable/setting_item_selector"
                android:drawableLeft="@drawable/icon_hb"
                android:drawablePadding="10dp"
                android:drawableRight="@drawable/right"
                android:gravity="center_vertical"
                android:paddingLeft="16dp"
                android:paddingRight="16dp"
                android:text="@string/my_red_pager"
                android:textSize="16dp" />

            <View
                android:layout_width="fill_parent"
                android:layout_height="10dp"
                android:background="@color/black2" />

            <RelativeLayout
                android:id="@+id/layout_zhaq"
                android:layout_width="fill_parent"
                android:layout_height="50dp" >

                <TextView
                    android:id="@+id/how_old6"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:background="@drawable/setting_item_selector"
                    android:drawableLeft="@drawable/img_zhaq"
                    android:drawablePadding="10dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="16dp"
                    android:text="@string/zhaq"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/how_old6"
                    android:layout_alignBottom="@+id/how_old6"
                    android:layout_toLeftOf="@+id/tv_safelevel"
                    android:text="@string/aqdj"
                    android:textSize="14dp" />

                <TextView
                    android:id="@+id/tv_safelevel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/how_old6"
                    android:layout_alignBottom="@+id/how_old6"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="16dp"
                    android:paddingLeft="4dp"
                    android:textColor="#FF0000"
                    android:textSize="14dp" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:background="@color/black2" />

            <RelativeLayout
                android:id="@+id/layout_ssmm"
                android:layout_width="match_parent"
                android:layout_height="50dp" >

                <TextView
                    android:id="@+id/how_old2"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    android:background="@drawable/setting_item_selector"
                    android:drawableLeft="@drawable/img_ssmm"
                    android:drawablePadding="10dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="16dp"
                    android:text="@string/ssmm"
                    android:textSize="16dp" />

                <TextView
                    android:id="@+id/tv_ssmm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/how_old2"
                    android:layout_alignBottom="@+id/how_old2"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="16dp"
                    android:text="@string/ssmm_msg"
                    android:textSize="14dp" />
            </RelativeLayout>
            <View
                android:layout_width="fill_parent"
                android:layout_height="1.0dp"
                android:background="@color/black2" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>
</LinearLayout>